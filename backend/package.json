{
  "name": "romero-stock-control",
  "version": "1.0.0",
  "description": "Sistema de control de stock para Romero Panificados",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:security": "NODE_ENV=test jest tests/security.test.js",
    "lint": "echo 'Linter no configurado. Considerar agregar ESLint'",
    "prestart": "node -e \"require('./utils/validateEnv').validateEnvironment()\"",
    "render-build": "npm install",
    "render-start": "npm start"
  },
  "keywords": [
    "stock",
    "inventario",
    "romero"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chartjs-node-canvas": "^4.1.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.14.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
